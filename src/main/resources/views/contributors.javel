<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/css/style.css">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Document</title>
</head>

<body id="body" class="toggleBG">
    <div id="wholePage" class="">
        <div class="topnav" id="topnavChad">
            <a href="/home">Home</a>
            <a href="/partners">Partners</a>
            <a class="active" href="/contributors">Contributors</a>
            <a href="/about">About</a>
        </div>
        <!-- Slideshow container -->
        <div class="slideshow-container">
            <!-- Full-width images with number and caption text -->
            <div class="mySlides fade">
                <div class="numbertext">1 / 4</div>
                <img src="/img/chad1.png" style="width:100%">
                <div class="text">{{contributor1.firstname}}</br>{{contributor1.lastname}}</div>
            </div>

            <div class="mySlides fade">
                <div class="numbertext">2 / 4</div>
                <img src="/img/chad2.png" style="width:100%">
                <div class="text">{{contributor2.firstname}}</br>{{contributor2.lastname}}</div>
            </div>

            <div class="mySlides fade">
                <div class="numbertext">3 / 4</div>
                <img src="/img/chad3.png" style="width:100%">
                <div class="text">{{contributor3.firstname}}</br>{{contributor3.lastname}}</div>
            </div>

            <div class="mySlides fade">
                <div class="numbertext">4 / 4</div>
                <img src="/img/chad4.png" style="width:100%">
                <div class="text">{{contributor4.firstname}}</br>{{contributor4.lastname}}</div>
            </div>

            <!-- Next and previous buttons -->
            <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
            <a class="next" onclick="plusSlides(1)">&#10095;</a>
        </div>
        <br>

        <!-- The dots/circles -->
        <div style="text-align:center">
            <span class="dot" onclick="currentSlide(1)"></span>
            <span class="dot" onclick="currentSlide(2)"></span>
            <span class="dot" onclick="currentSlide(3)"></span>
            <span class="dot" onclick="currentSlide(4)"></span>
        </div>

        <footer id="footer">

        </footer>
    </div>
    <script src="/js/contributors.js"></script>
</body>

</html>